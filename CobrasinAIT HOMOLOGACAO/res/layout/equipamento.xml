<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:weightSum="1" >
    
    <TextView
        android:id="@+id/lblSelecione"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Lab1Tela13" />

    <EditText
        android:id="@+id/edEquipamento"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:inputType="text"
        android:maxLength="30" />

    <TextView
        android:id="@+id/lblTotalEnquadramento"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Lab2Tela13" />

    <EditText
        android:id="@+id/edMedicaoReg"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:inputType="numberDecimal|text"
        android:maxLength="9" />

    <TextView
        android:id="@+id/lblTotalMdsAdm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Lab3Tela13" />

    <EditText
        android:id="@+id/edMedicaoCon"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:inputType="numberDecimal|text" >

        <requestFocus />
    </EditText>

    <TextView
        android:id="@+id/lblTotalPais"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Lab4Tela13" />

    <EditText
        android:id="@+id/edLimiteReg"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:inputType="numberDecimal|text"
        android:maxLength="9" />

    <Button
        android:id="@+id/btGravaEquip"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="@string/Lab2Tela9" />
    
</LinearLayout>
