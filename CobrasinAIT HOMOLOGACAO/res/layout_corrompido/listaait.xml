<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="fill_parent"
    android:orientation="vertical" >

    <ListView
        android:id="@+id/listaAit"
        android:layout_width="fill_parent"
        android:layout_height="238dp"
        android:layout_marginRight="10dp"
        android:layout_weight="0.94" >
    </ListView>

    <Button
        android:id="@+id/btNovoAit"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="@string/Lab1Tela8" />
    
        <Button
        android:id="@+id/btnConsutarPlacaLISTA"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="Consultar Placa" />
    
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="44dp" >

        <Button
            android:id="@+id/btPrintAll"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0.66"
            android:text="@string/Lab5Tela8" />

        <Button
            android:id="@+id/btTransmite"
            android:layout_width="138dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.66"
            android:text="@string/Lab3Tela8" />
    </LinearLayout>

    <Button
        android:id="@+id/btEncerraAit"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="@string/Lab2Tela8" />

</LinearLayout>